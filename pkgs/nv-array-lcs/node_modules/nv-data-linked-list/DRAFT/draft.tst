

const {
  init,
  peek,
  remove,
  append,
  isEmpty
} = require("./index")

const uuid = require("uuid")

class TimersList {
    constructor(expiry, msecs){
        this._idleNext = this; 
        this._idlePrev = this; 
        this.expiry = expiry;
        this.id = uuid.v4();
        this.msecs = msecs;
        this.priorityQueuePosition = null;
    }
}

var tl0 = new TimersList(process.hrtime()[0],5000)
var tl1 = new TimersList(process.hrtime()[0],5000)

init(tl0) 
init(tl0)
append(tl0,tl1)

